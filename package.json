{
  "name": "@placemarkio/polyline",
  "version": "1.1.1",
  "description": "encode and decode polylines",
  "scripts": {
    "test": "vitest run",
    "test:ci": "vitest run",
    "lint": "eslint",
    "doc": "typedoc --options ./typedoc.json",
    "prepare": "zshy"
  },
  "zshy": "./src/index.ts",
  "peerDependencies": {
    "@types/geojson": "*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/placemark/polyline.git"
  },
  "author": "Tom MacWright",
  "license": "MIT",
  "dependencies": {
    "@types/geojson": "*"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "eslint": "^8.24.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.5.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.15",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9",
    "zshy": "^0.4.4"
  },
  "keywords": [
    "polyline",
    "geojson"
  ],
  "files": [
    "**/*.js",
    "**/*.mjs",
    "**/*.cjs",
    "**/*.d.ts",
    "**/*.d.mts",
    "**/*.d.cts"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  }
}
